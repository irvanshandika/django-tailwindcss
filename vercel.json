{
  "builds": [
    {
      "src": "mysite/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "theme/static/styles.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "theme/static/styles.css"
    },
    {
      "src": "/(.*)",
      "dest": "mysite/wsgi.py"
    }
  ]
}
